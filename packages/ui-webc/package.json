{
  "name": "@scouterna/ui-webc",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-webc/ui-webc.esm.js",
  "exports": {
    ".": {
      "import": "./dist/scout/scout.esm.js",
      "require": "./dist/scout/scout.cjs.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "dev": "rimraf lab/output && pnpm run lab:tailwind && concurrently --names docs,sten \"pnpm run lab:generate\" \"pnpm run lab:server\"",
    "lab:tailwind": "tailwindcss -i ./lab/style.css -o ./lab/output/style.css",
    "lab:generate": "node lab/generate-lab.mts",
    "lab:server": "stencil build --dev --watch --serve"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@scouterna/ui-tailwind-theme": "workspace:^",
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.2.0",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react-dom": "^19.1.7",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.9",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "mdx-bundler": "^10.1.1",
    "puppeteer": "^24.3.0",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.12"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  }
}