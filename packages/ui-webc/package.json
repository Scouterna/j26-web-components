{
  "name": "@scouterna/ui-webc",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-webc/ui-webc.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ui-webc/ui-webc.esm.js",
      "require": "./dist/ui-webc/ui-webc.cjs.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "storybook dev -p 6006 --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@scouterna/design-tokens": "workspace:^"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/core": "^4.38.1",
    "@stencil/storybook-plugin": "^0.4.2",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-links": "9.1.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss-preset-env": "^10.4.0",
    "puppeteer": "^24.3.0",
    "storybook": "9.1.10"
  },
  "license": "MIT"
}