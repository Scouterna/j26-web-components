{
  "name": "@scouterna/ui-webc",
  "version": "0.0.1",
  "type": "module",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-webc/ui-webc.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ui-webc/ui-webc.esm.js",
      "require": "./dist/ui-webc/ui-webc.cjs.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./box-component": {
      "import": "./dist/components/box-component.js",
      "types": "./dist/components/box-component.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:watch": "stencil build --watch",
    "dev": "stencil build --watch --serve",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "plop": "plop"
  },
  "dependencies": {
    "@scouterna/design-tokens": "workspace:^",
    "class-variance-authority": "^0.7.1",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/core": "^4.38.1",
    "@stencil/storybook-plugin": "^0.4.2",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-links": "9.1.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "iconoir": "^7.11.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "plop": "^4.0.4",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.4.0",
    "puppeteer": "^24.3.0",
    "storybook": "9.1.10"
  },
  "license": "MIT"
}
