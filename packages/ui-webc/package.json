{
  "name": "@scouterna/ui-webc",
  "version": "2.2.1",
  "type": "module",
  "description": "Scouterna Design System Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-webc/ui-webc.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ui-webc/ui-webc.esm.js",
      "require": "./dist/ui-webc/ui-webc.cjs.js"
    },
    "./style.css": {
      "import": "./dist/ui-webc/ui-webc.css",
      "require": "./dist/ui-webc/ui-webc.css"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Scouterna/j26-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --watch",
    "serve": "stencil build --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@scouterna/design-tokens": "workspace:^",
    "@stencil/react-output-target": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "iconoir": "^7.11.0",
    "@tabler/icons": "^3.35.0"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/core": "^4.38.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.5.0",
    "puppeteer": "^24.32.0"
  },
  "license": "MIT"
}
